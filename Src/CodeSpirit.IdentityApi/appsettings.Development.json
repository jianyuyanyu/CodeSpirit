{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "ConnectionStrings": {
    "identity-api": "Server=(localdb)\\mssqllocaldb;Database=codespirit-identity;Trusted_Connection=True;MultipleActiveResultSets=true;Packet Size=512"
  },
  "ConfigCenter": {
    "ServiceUrl": "http://localhost:62145",
    "AppId": "identity",
    "AppSecret": "",
    "Environment": "Development",
    "AutoRegisterApp": true,
    "AppName": "Your App Name",
    "PollIntervalSeconds": 60,
    "UseSignalR": true,
    "EnableLocalCache": true,
    "LocalCacheDirectory": ".config-cache",
    "CacheExpirationMinutes": 1440,
    "PreferCache": false
  }
}
