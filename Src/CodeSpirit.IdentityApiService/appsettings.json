{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "PagesConfiguration": {
    "Pages": [
      {
        "label": "控制台",
        "url": "/",
        "redirect": "/page/app"
      },
      {
        "label": "app",
        "url": "/page/app",
        "schema": {
          "type": "page",
          "body": {
            "type": "iframe",
            "src": "https://localhost:17109/",
            "height": "100%"
          }
        }
      },
      {
        "label": "用户中心",
        "children": [
          {
            "label": "用户统计",
            "schemaApi": "/test.json"
          },
          {
            "label": "用户管理",
            "schemaApi": "https://localhost:17134/api/amis/users",
            "url": "/page/identity/users"
          },
          {
            "label": "角色管理",
            "schemaApi": "https://localhost:17134/api/amis/roles",
            "url": "/page/identity/roles"
          },
          {
            "label": "权限管理",
            "schemaApi": "https://localhost:17134/api/amis/permissions"
          },
          {
            "label": "登录日志",
            "schemaApi": "jsonp:/pages/LoginLogs.js?callback=jsonpCallback"
          },
          {
            "label": "操作日志",
            "schemaApi": "jsonp:/pages/OperationLogs.js?callback=jsonpCallback"
          },
          {
            "label": "系统设置",
            "schemaApi": "/pages/system-settings.json"
          }
        ]
      }
    ]
  }
}
