{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Services": {
    "config": {
      "https": [ "https://localhost:62144" ]
    },
    "identity": {
      "https": [ "https://localhost:17134" ]
    }
  },
  "Navigation": {
    "externalSystem": {
      "Name": "externalSystem",
      "Title": "外部系统",
