{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Aspire": {
    "Seq": {
      "DisableHealthChecks": true,
      "ServerUrl": "http://seq:5341"
    }
  },
  "Navigation": {
    "externalSystem": {
      "Name": "externalSystem",
      "Title": "外部系统",
      "Path": "/external",
      "Icon": "fa-solid fa-link",
      "Children": [
        {
          "Name": "system1",
          "Title": "deepseek",
          "Path": "/externalSystem/system1",
          "Link": "https://chat.deepseek.com/",
          "Icon": "fa-solid fa-external-link",
          "IsExternal": true,
          "Target": "_blank"
        }
      ]
    },
    "monitor": {
      "Name": "monitor",
      "Title": "监控中心",
      "Icon": "fa-solid fa-chart-line",
      "Path": "/monitor",
      "Children": [
        {
          "Name": "logs",
          "Title": "日志服务",
          "Link": "https://codespirit-seq.xin-lai.com/",
          "Path": "/monitor/logs",
          "Icon": "fa-solid fa-file-lines",
          "IsExternal": true,
          "Target": "_blank"
        },
        {
          "Name": "console-logs",
          "Title": "控制台日志",
          "Path": "/monitor/console-logs",
          "Link": "${aspire_dashboard}/consolelogs",
          "Icon": "fa-solid fa-terminal",
          "IsExternal": true,
          "Target": "_self"
        }
      ]
    }
  }
}